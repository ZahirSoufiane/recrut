	  "bundle": true,
	  "js": {"compile": true, "minify": true},
	  "css": {"minify": true},
	  "html": {"minify": true},
	  "addServiceWorker": true 
	}],
  "shell": "resources/elements-company.html",
  "fragments": [
  	"resources/html/ym-comp-profile.html",
    "resources/html/ym-comp-dashboard.html",
    "resources/html/ym-agenda.html",
