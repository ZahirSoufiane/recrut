{
  "entrypoint": "adm.html",
  "builds": [{
	  "bundle": true,
	  "js": {"compile": true, "minify": true},
	  "css": {"minify": true},
	  "html": {"minify": true},
	  "addServiceWorker": true 
	}],
  "shell": "resources/elements-admin.html",
  "fragments": [
  	"resources/html/admin/ym-adm-categories.html",
    "resources/html/admin/ym-adm-couples.html",
    "resources/html/admin/ym-adm-companies.html",
    "resources/html/admin/ym-adm-companies-details.html"
  ],
  "sources": [
   "resources/src/**/*",
   "resources/css/**/*",
   "resources/html/admin/data/*",
   "resources/images/**/*",
   "resources/img/**/*",
   "resources/js/*",
   "resources/js/cal/*",
   "resources/js/countdown/*",
   "resources/html/include/**/*",
   "resources/bower.json"
  ],
  "extraDependencies": [
    "resources/bower_components/webcomponentsjs/webcomponents-lite.min.js"
  ]
}
